<Window x:Class="Ex05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ex05"
        mc:Ignorable="d"
        Title="Dicionário" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,20,0,0" TextWrapping="Wrap" Text="Chave" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="146,20,0,0" TextWrapping="Wrap" Text="Valor" VerticalAlignment="Top"/>
        <TextBox x:Name="chave" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="valor" HorizontalAlignment="Left" Height="23" Margin="146,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="126"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="250" Margin="10,131,0,0" VerticalAlignment="Top" Width="550"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Width="75" Click="ButtonClick_Add"/>
        <Button Content="Clear" HorizontalAlignment="Left" Margin="277,90,0,0" VerticalAlignment="Top" Width="75" Click="ButtonClick_Clear"/>
        <Button Content="Contains" HorizontalAlignment="Left" Margin="186,90,0,0" VerticalAlignment="Top" Width="75" Click="ButtonClick_Contains"/>
        <Button Content="Remove" HorizontalAlignment="Left" Margin="98,90,0,0" VerticalAlignment="Top" Width="75" Click="ButtonClick_Remove"/>

    </Grid>
</Window>
